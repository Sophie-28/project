int toint(string numstr);
int main(void)
{
ifstream file2("pro.txt");
if(!file2.is_open())
{
cout<<"Failed to open";
}
else{
cout<<"File opened";
}

		string b,a,p;
		int b2,a2,p2;
		string line;
		while(file2.good())
		{
			while(getline(file2, line))
			{
				stringstream ss(line);
				getline(ss,b,':');
				b2=toint(b);
				getline(ss,a,':');
			    a2=toint(a);
				getline(ss,p,':');
				p2=toint(p);
				
	cout<<b2;
	cout<<a2;
	cout<<p2<<"\n";
			}
	
	}
	

/*	while(file2.good())
